     )}\n        </div>\n      </Content>\n      <Footer style={{ textAlign: \"center\" }}>\n        Creado por Nicol√°s Morales\n      </Footer>\n    </Layout>\n  );\n};\nexport default withRouter(Login);\n","/media/ShadowsData/www/React/maxcode/src/client/pages/index.js",[],"/media/ShadowsData/www/React/maxcode/src/admin/components/projects/List.js",[],"/media/ShadowsData/www/React/maxcode/src/admin/context/authContext.js",[],"/media/ShadowsData/www/React/maxcode/src/client/components/Gallery.js",["115","116","117","118"],"/media/ShadowsData/www/React/maxcode/src/admin/components/listItems.js",[],"/media/ShadowsData/www/React/maxcode/src/client/components/Sidebar.js",[],"/media/ShadowsData/www/React/maxcode/src/client/components/Loading.js",[],"/media/ShadowsData/www/React/maxcode/src/client/components/PassionChip.js",[],"/media/ShadowsData/www/React/maxcode/src/admin/components/Cargando.js",[],"/media/ShadowsData/www/React/maxcode/src/client/components/Template.js",[],"/media/ShadowsData/www/React/maxcode/src/client/components/Profiles.js",["119","120","121"],{"ruleId":"122","replacedBy":"123"},{"ruleId":"124","replacedBy":"125"},{"ruleId":"122","replacedBy":"126"},{"ruleId":"124","replacedBy":"127"},{"ruleId":"128","severity":1,"message":"129","line":24,"column":1,"nodeType":"130","endLine":30,"endColumn":3},{"ruleId":"122","replacedBy":"131"},{"ruleId":"124","replacedBy":"132"},{"ruleId":"133","severity":1,"message":"134","line":47,"column":15,"nodeType":"135","endLine":51,"endColumn":17},{"ruleId":"136","severity":1,"message":"137","line":9,"column":10,"nodeType":"138","messageId":"139","endLine":9,"endColumn":20},{"ruleId":"136","severity":1,"message":"140","line":4,"column":10,"nodeType":"138","messageId":"139","endLine":4,"endColumn":22},{"ruleId":"122","replacedBy":"141"},{"ruleId":"124","replacedBy":"142"},{"ruleId":"136","severity":1,"message":"140","line":4,"column":10,"nodeType":"138","messageId":"139","endLine":4,"endColumn":22},{"ruleId":"136","sever